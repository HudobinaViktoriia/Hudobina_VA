import java.util.Scanner;

public class ClosestSquare {
    public static void main(String[] args) {
        // Создаем объект Scanner для считывания ввода пользователя
        Scanner scanner = new Scanner(System.in);

        // Запрашиваем у пользователя ввод числа
        System.out.print("Введите число: ");
        int inputNumber = scanner.nextInt();

        // Находим ближайший квадратный корень, округленный вниз и вверх
        int sqrtFloor = (int) Math.floor(Math.sqrt(inputNumber));
        int sqrtCeil = (int) Math.ceil(Math.sqrt(inputNumber));

        // Вычисляем квадраты этих чисел
        int squareFloor = sqrtFloor * sqrtFloor;
        int squareCeil = sqrtCeil * sqrtCeil;

        // Сравниваем квадраты с исходным числом
        int closestSquare;
        if (Math.abs(inputNumber - squareFloor) <= Math.abs(inputNumber - squareCeil)) {
            closestSquare = sqrtFloor;
        } else {
            closestSquare = sqrtCeil;
        }

        // Выводим результат
        System.out.println("Число, квадрат которого ближе всего к " + inputNumber + " это " + closestSquare);

        // Закрываем Scanner
        scanner.close();
    }
}
